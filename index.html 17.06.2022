          handleBlur() {
            this.isErrorMessage = false;
          },
          sendSRM() {
            if (
              this.newPerson.name != "" &&
              this.newPerson.phone.length >= 12
            ) {
              axios
                .post(
                  "https://landingleads.bi.group",
                  {
                    clientName: this.newPerson.name,
                    clientPhoneNumber: this.newPerson.phone,
                    url: this.url,
                    realEstateId: this.idBack,
                    googleClientId: this.cookieGA,
                    companyName: this.companyName,
                    callDate: this.timestamp,
                    attractionChannel: "null",
                    clientInterest: "null",
                    siteApplicationType: this.siteApplicationType,
                    packageSmartRemont: this.packageSmartRemont,
                    costSmartRemont: this.costSmartRemont,
                    propertyType: this.propertyType,
                    other: this.other,
                    belongsToDepartment: this.belongsToDepartment,
                  },
                  {
                    headers: {
                      Authorization: "c2l0ZTpASiNeeVFGcEQwaSp4OGJNbSU=",
                      "Content-Type": "application/json",
                      "Postman-Token":
                        "099cb621-876d-4e23-915b-e90123655b76,19270599-5a99-474b-adf9-7f5e901135b1",
                      "cache-control": "no-cache",
                    },
                  }
                )
                .then(function (response) {
                  console.log(response.data.message);
                })
                .catch((error) => {
                  console.log(error);
                  this.isErrorMessage = true;
                });
              this.isSuccessMessage = true;
            } else {
              this.isErrorMessage = true;
            }
          },
          getNumber() {
            let clearMask = "+" + this.phoneMask.replace(/\D/g, "");
            if (this.phoneMask.length === 18) {
              this.newPerson.phone = clearMask;
            } else if (this.phoneMask.length === 19) {
              this.newPerson.phone = clearMask.substring(
                0,
                clearMask.length - 1
              );
            }
          },
          getCookie() {
            function getCookie(name) {
              let matches = document.cookie.match(
                new RegExp(
                  "(?:^|; )" +
                    name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, "\\$1") +
                    "=([^;]*)"
                )
              );
              return matches ? decodeURIComponent(matches[1]) : undefined;
            }
            let cookieGA = getCookie("_ga");
            this.cookieGA = cookieGA;
          },
          getData() {
            var now = new Date();
            function formatDate(date) {
              var dd = date.getDate();
              if (dd < 10) dd = "0" + dd;
              var mm = date.getMonth() + 1;
              if (mm < 10) mm = "0" + mm;
              var yy = date.getFullYear();
              if (yy < 10) yy = "0" + yy;
              return dd + "." + mm + "." + yy;
            }
            var nowFormat = formatDate(now);
            this.timestamp = new Date(
              nowFormat.split(".").reverse().join(".")
            ).getTime();
          },
        },
      });
