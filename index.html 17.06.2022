    <script type="text/javascript">
      const footerId = "bi-footer__contacts";
      const selectCityElId = "bi-footer__select-city";
      let cities = [];
      let cityIndex = 0;
      let adminContacts;
      let coords = [51.128207, 71.43042];
      let cityName;
      let cityElements;
      let myMap1;
      function setCityIndex(index) {
        cityIndex = index;
        cityName = cities[cityIndex];
        renderFooterElems();
      }
      function renderFooterElems() {
        let el = document.getElementById(footerId);
        cityElements = adminContacts.filter(
          (item) => item.city.title === cities[cityIndex]
        );
        const renderHtml = cityElements
          .map((item) => {
            const address = item.address
              .replaceAll("<br>", "")
              .replaceAll("<strong>", "")
              .replaceAll("</strong>", "")
              .replaceAll(
                "color:hsl(0,0%,0%);background-color:rgb(253,253,253);",
                ""
              )
              .replaceAll("color:hsl(0,0%,0%);", "")
              .replace(/\<p>&nbsp;<\/p>/g, "")
              .replaceAll(
                '<a href=""><span class="text-small" style="color:hsl(210,75%,60%);">Показать на карте</span></a>',
                `<span data-latitude="${item.latitude}" data-longitude="${item.longitude}"onclick="yandexAddressMapFocus(event)" class="text-small map-link" style="color:hsl(210,75%,60%);">Показать на карте </span> `
              );
            return `<div class="contact-block-50"> ${address} </div>`;
          })
          .join("");
        el.innerHTML = renderHtml;
      }
      async function getFooterInfo() {
        let { data } = await axios.get("https://admin.bi.group/json/contacts");
        adminContacts = data.data;
        cities = [...new Set(data.data.map((item) => item.city.title))];
        renderFooter();
        renderFooterElems();
      }
      async function renderFooter(zoom) {
        let selectCityEl = document.getElementById(selectCityElId);
        const selectCityElements = cities
          .map(
            (item, i) =>
              `<span class="city contact-tab-menu-link ${
                i === cityIndex && "bi-footer__select-city--active"
              }" onclick="setCityIndex(${i})">${item}</span>`
          )
          .join("");
        selectCityEl.innerHTML = selectCityElements;
        cityElements = adminContacts.filter(
          (item) => item.city.title === cities[cityIndex]
        );
        ymaps.ready(() => {
          init1(zoom);
        });
      }
      function init1(zoom) {
        myMap1 = new ymaps.Map("map1", {
          center: coords,
          zoom: zoom || 12,
          controls: [],
        });
        var zoomControl = new ymaps.control.ZoomControl({
          options: {
            position: {
              right: "14px",
              top: document.body.clientWidth > 600 ? "215px" : "100px",
            },
          },
        });
        myMap1.controls.add(zoomControl);
        myMap1.behaviors.disable("scrollZoom");
        nurCollection = new ymaps.GeoObjectCollection(null, {
          preset: "islands#yellowIcon",
          iconLayout: "default#image",
          iconImageHref: "https://portal.bi-group.org/Content/img/logo.svg",
          iconImageSize: [52, 52],
        });
        cityElements.forEach((item) => {
          nurCollection.add(
            new ymaps.Placemark([item.latitude, item.longitude], {
              hintContent: item.address,
            })
          );
        });
        myMap1.geoObjects.add(nurCollection);
        document
          .querySelector("#bi-footer__select-city")
          .addEventListener("click", (e) => {
            e.preventDefault();
            ymaps.geocode(cityName).then(function (res) {
              coords = res.geoObjects.get(0).geometry.getCoordinates();
              myMap1.setCenter(coords, 12, {
                duration: 400,
                checkZoomRange: true,
              });
            });
            cityElements.forEach((item) => {
              nurCollection.add(
                new ymaps.Placemark([item.latitude, item.longitude], {
                  hintContent: item.address,
                })
              );
            });
            const active = document.querySelectorAll(
              ".bi-footer__select-city--active"
            );
            if (active) {
              active.forEach((item) => {
                item.classList.remove("bi-footer__select-city--active");
              });
            }
            e.target.classList.add("bi-footer__select-city--active");
          });
      }
      function yandexAddressMapFocus(argument) {
        // body...
        const latitude = argument.target.getAttribute("data-latitude");
        const longitude = argument.target.getAttribute("data-longitude");
        let focus = [latitude, longitude];
        myMap1.setZoom(15);
        myMap1.setCenter(focus);
      }
      getFooterInfo();
    </script>
